{
  "name": "Sodam_Front_End",
  "version": "0.0.1",
  "private": true,
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "lint": "eslint .",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__|src",
    "test:logcat": "jest tests/logcat/boot_logcat.test.ts",
    "test:all": "npm run test:unit && npm run test:logcat",
    "test:skip": "node -e \"console.log('Skipping tests this session due to local resource constraints'); process.exit(0)\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:update": "jest --updateSnapshot",
    "gradle-clean": "cd android && ./gradlew clean && cd ..",
    "metro-reset": "npx react-native start --reset-cache",
    "pre-android": "npm run gradle-clean && npm run metro-reset",
    "android:jsc": "npm run gradle-clean && npm run metro-reset && npm run android",
    "android:clean": "powershell -ExecutionPolicy Bypass -File .\\scripts\\android-clean.ps1",
    "android:run": "npm run android:clean && npm run metro-reset && react-native run-android",
    "grant:all": "powershell -ExecutionPolicy Bypass -File .\\scripts\\grant-permissions.ps1",
    "clean:all": "npm run gradle-clean && rmdir /s /q node_modules && npm install",
    "diag:native": "powershell -ExecutionPolicy Bypass -File .\\scripts\\verify-native-modules.ps1 -Device auto -Out logs\\verify-native-modules.log",
    "perf:capture": "powershell -ExecutionPolicy Bypass -File .\\scripts\\collect-performance.ps1 -DurationSec 30 -IntervalMs 1000 -Out logs\\perf-collect.log",
    "run:iter": "powershell -ExecutionPolicy Bypass -File .\\scripts\\run-iter-suite.ps1 -Device auto -TimeoutSec 15",
    "sync:gradle": "powershell -ExecutionPolicy Bypass -File .\\scripts\\reproduce-gradle-sync.ps1",
    "build:android:debug": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-android.ps1 -BuildType debug -OutputFormat apk",
    "build:android:release": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-android.ps1 -BuildType release -OutputFormat apk",
    "build:android:bundle": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-android.ps1 -BuildType release -OutputFormat aab",
    "build:ios:sim": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-ios.ps1 -Platform simulator",
    "build:ios:device": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-ios.ps1 -Platform device",
    "build:web": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-web.ps1",
    "build:web:dev": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-web.ps1 -BuildType development",
    "build:web:serve": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-web.ps1 -Serve",
    "build:web:analyze": "powershell -ExecutionPolicy Bypass -File .\\scripts\\deploy-web.ps1 -Analyze",
    "build:all": "npm run build:android:release && npm run build:web",
    "predeploy": "npm run lint && npm run test:ci",
    "deploy:test": "npm run predeploy && npm run build:android:debug && npm run build:web"
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.22.0",
    "@react-native/new-app-screen": "0.81.0",
    "@react-navigation/native": "^6.1.17",
    "@react-navigation/native-stack": "^6.9.26",
    "@tanstack/react-query": "^5.85.0",
    "@types/react-native-vector-icons": "^6.4.18",
    "axios": "^1.4.0",
    "date-fns": "^2.30.0",
    "metro-config": "^0.83.1",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-geolocation-service": "^5.3.0",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-linear-gradient": "^2.8.3",
    "react-native-nfc-manager": "^3.14.0",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "^4.1.0",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.14.1",
    "react-native-svg": "^15.2.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-native-vector-icons": "^10.3.0",
    "react-native-worklets": "^0.5.0",
    "@react-native-community/datetimepicker": "^8.4.5"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/cli": "^11.3.10",
    "@react-native-community/cli-platform-android": "^11.3.10",
    "@react-native-community/cli-platform-ios": "^11.3.10",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.11",
    "@types/react-test-renderer": "^19.1.0",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.0.4"
  },
  "overrides": {
    "inflight": "^1.0.6"
  }
}
