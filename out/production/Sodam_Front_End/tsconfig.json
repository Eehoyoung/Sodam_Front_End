{
  "extends": "@react-native/typescript-config",
  "compilerOptions": {
    "noEmit": true
  },
  "include": [
    "src/**/*",
    "App.tsx"
  ],
  "exclude": [
    "**/__tests__/**",
    "android",
    "ios",
    "out",
    "node_modules",
    "**/*.test.ts",
    "**/*.test.tsx",
    "**/*.spec.ts",
    "**/*.spec.tsx"
  ]
}
