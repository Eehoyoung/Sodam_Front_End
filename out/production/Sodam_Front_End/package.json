{
  "name": "Sodam_Front_End",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "expo run:ios",
    "lint": "eslint .",
    "start": "react-native start",
    "test": "jest",
    "test:skip": "node -e \"console.log('Skipping tests this session due to local resource constraints'); process.exit(0)\"",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --coverage --watchAll=false",
    "test:update": "jest --updateSnapshot",
    "gradle-clean": "cd android && ./gradlew clean && cd ..",
    "metro-reset": "npx react-native start --reset-cache",
    "pre-android": "npm run gradle-clean && npm run metro-reset",
    "android:jsc": "npm run gradle-clean && npm run metro-reset && npm run android",
    "clean:all": "npm run gradle-clean && rmdir /s /q node_modules && npm install",
    "diag:native": "powershell -ExecutionPolicy Bypass -File .\\scripts\\verify-native-modules.ps1 -Device auto -Out logs\\verify-native-modules.log",
    "validate-modules": "npm run diag:native",
    "perf:capture": "powershell -ExecutionPolicy Bypass -File .\\scripts\\collect-performance.ps1 -DurationSec 30 -IntervalMs 1000 -Out logs\\perf-collect.log",
    "run:iter": "powershell -ExecutionPolicy Bypass -File .\\scripts\\run-iter-suite.ps1 -Device auto -TimeoutSec 15"
  },
  "dependencies": {
    "@expo/vector-icons": "^14.1.0",
    "@react-native-async-storage/async-storage": "^2.2.0",
    "@react-native/new-app-screen": "0.81.0",
    "@react-navigation/native": "^7.1.17",
    "@react-navigation/stack": "^7.4.7",
    "@tanstack/react-query": "^5.85.0",
    "axios": "^1.11.0",
    "date-fns": "^4.1.0",
    "expo": "~53.0.20",
    "expo-asset": "^11.1.7",
    "expo-constants": "^17.1.7",
    "expo-font": "^13.3.2",
    "expo-modules-core": "^2.5.0",
    "react": "19.1.0",
    "react-native": "0.81.0",
    "react-native-chart-kit": "^6.12.0",
    "react-native-geolocation-service": "^5.3.1",
    "react-native-gesture-handler": "^2.28.0",
    "react-native-nfc-manager": "^3.16.2",
    "react-native-permissions": "^5.4.2",
    "react-native-reanimated": "^3.6.0",
    "react-native-safe-area-context": "^5.6.0",
    "react-native-screens": "^4.14.1"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@babel/preset-env": "^7.25.3",
    "@babel/runtime": "^7.25.0",
    "@react-native-community/cli": "20.0.0",
    "@react-native-community/cli-platform-android": "20.0.0",
    "@react-native-community/cli-platform-ios": "20.0.0",
    "@react-native/babel-preset": "0.81.0",
    "@react-native/eslint-config": "0.81.0",
    "@react-native/metro-config": "0.81.0",
    "@react-native/typescript-config": "0.81.0",
    "@types/jest": "^29.5.13",
    "@types/react": "^19.1.0",
    "@types/react-test-renderer": "^19.1.0",
    "eslint": "^8.19.0",
    "jest": "^29.6.3",
    "prettier": "2.8.8",
    "react-test-renderer": "19.1.0",
    "typescript": "^5.8.3"
  },
  "engines": {
    "node": ">=18"
  },
  "overrides": {
    "inflight": "^1.0.6"
  }
}
